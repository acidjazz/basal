[
    {
        "name": "basal-${deploy_id}",
        "image": "${docker_username}/basal:${version}",
        "cpu": 256,
        "memory": 768,
        "essential": true,
        "portMappings": [
            {
                "hostPort": 80,
                "containerPort": 80,
                "protocol": "tcp"
            }
        ],
        "links": [],
        "command": [],
        "entryPoint": [],
        "environment": [
            {
                "name": "APP_KEY",
                "value": "$app_key"
            },
            {
                "name": "DB_PASSWORD",
                "value": "$db_password"
            },
            {
                "name": "S3_KEY",
                "value": "$s3_key"
            },
            {
                "name": "S3_SECRET",
                "value": "$s3_secret"
            },
            {
                "name": "AUTH_GOOGLE_ID",
                "value": "$auth_google_id"
            },
            {
                "name": "AUTH_GOOGLE_SECRET",
                "value": "$auth_google_secret"
            }
        ],
        "mountPoints": []
    }
]
